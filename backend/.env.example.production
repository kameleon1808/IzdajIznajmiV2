APP_NAME=IzdajIznajmi
APP_ENV=production
APP_KEY=base64:GENERATE_WITH_ARTISAN_KEY_GENERATE
APP_DEBUG=false
APP_URL=https://api.izdaji.example
APP_VERSION=1.0.0

FRONTEND_URL=https://app.izdaji.example
FRONTEND_URLS=https://app.izdaji.example
SANCTUM_STATEFUL_DOMAINS=app.izdaji.example
SESSION_DOMAIN=.izdaji.example
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=lax

LOG_CHANNEL=stack
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=izdaji
DB_USERNAME=forge
DB_PASSWORD=CHANGE_ME

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=true

QUEUE_CONNECTION=database
QUEUE_FAILED_DRIVER=database-uuids

CACHE_STORE=database

FILESYSTEM_DISK=public

MAIL_MAILER=smtp
MAIL_HOST=127.0.0.1
MAIL_PORT=587
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@izdaji.example
MAIL_FROM_NAME="${APP_NAME}"

GEOCODER_DRIVER=nominatim
GEOCODER_NOMINATIM_URL=https://nominatim.openstreetmap.org/search
GEOCODER_NOMINATIM_EMAIL=ops@izdaji.example
GEOCODER_NOMINATIM_COUNTRIES=
GEOCODER_NOMINATIM_RATE_LIMIT_MS=1200
GEOCODER_NOMINATIM_TIMEOUT=8
GEOCODER_SUGGEST_DRIVER=nominatim

SEARCH_MAX_RADIUS_KM=50
SEARCH_MAX_MAP_RESULTS=300
IMAGE_OPTIMIZE=true
IMAGE_MAX_WIDTH=1600
IMAGE_WEBP_QUALITY=80

SENTRY_DSN=
